{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "dashmark_pure",
            "request": "launch",
            "type": "dart",
            "env": {
                "RUST_BACKTRACE": "1",
                "RUST_LOG": "debug",
            }
        },
        {
            "name": "dashmark_pure (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "env": {
                "RUSTFLAGS": "-O -C target-cpu=native -C target-feature=+ssse3,+sse4.1,+sse4.2,+avx"
            }
        },
        {
            "name": "dashmark_pure @ aarch64 (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "env": {
                "RUSTFLAGS": "-O -C target-feature=+neon,+fp-armv8,+sve"
            }
        },
        {
            "name": "dashmark_pure (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "env": {
                "RUSTFLAGS": "-O -C target-cpu=native -C target-feature=+ssse3,+sse4.1,+sse4.2,+avx"
            }
        }
    ]
}